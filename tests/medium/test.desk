FUNC LABEL 0
    MOV STK 1 VAL 5 // A = 5
    REF STK 2 STK 1 // B = &A
    REF STK 3 PTR 2 // C = *(B) so C = *(&A) so C = A
    MOV REG 0 STK 3 // REG 0 = C = A = 5
    MOV REG 1 STK 2 // REG 1 = &A = 255
    CAL VAL 7
    PRINT REG 0 // 5
    PRINT REG 1 // 255
    RET
FUNC LABEL 7
    MOV REG 2 VAL 1 // REG 2 = 1
    NOT REG 1 // REG 1 = 0
    ADD REG 1 REG 2 // REG 1 = 1
    ADD REG 0 REG 1 // REG 0 = 6
    EQU REG 0 //
    PRINT REG 0
    RET
